# Server
ORCHESTRATOR_TRANSPORT=http          # http | stdio
ORCHESTRATOR_PORT=8080
ORCHESTRATOR_LOG_LEVEL=info          # debug | info | warning | error

# Server configuration file
SERVER_CONFIG_PATH=server_config.json  # Path to server config JSON file

# Storage backend
STORAGE_BACKEND=memory          # memory | redis
REDIS_URL=redis://localhost:6379

# Security
TOOL_CALL_TIMEOUT=30            # Seconds, per downstream call
MAX_REQUEST_SIZE_KB=512         # Input size limit for tool arguments
RATE_LIMIT_RPM=120